{
    "name": "cclark61/php-rpi-ws2812",
    "desc": "PHP Raspberry PI Client",
    "version": "0.1.0",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "phpopenfw/phpopenfw3" : "dev-master",
    },
    "require-dev": {},
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/phpopenfw/phpOpenFW3.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cclark61\\RPi\\WS2812\\": "src"
        }
    },
    "scripts-dev": {
        "post-install-cmd": [
            "vendor/bin/security-checker security:check"
        ],
        "post-update-cmd": [
            "vendor/bin/security-checker security:check"
        ]
    }
}
